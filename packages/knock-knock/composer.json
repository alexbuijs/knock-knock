{
  "name": "klopvaart/knock-knock",
  "type": "wordpress-theme",
  "authors": [
    {
      "name": "Klopvaart Intranet Team",
      "email": "internet@klopvaart.nl"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "require": {
    "php": "^7.3 || ^8.0",
    "nesbot/carbon": "^2.31",
    "timber/timber": "^2.0",
    "symfony/validator": "^6.0",
    "symfony/property-access": "^6.0",
    "symfony/mime": "^6.0",
    "symfony/dependency-injection": "^6.0",
    "symfony/asset": "^6.0"
  },
  "prefer-stable": true,
  "config": {
    "platform": {
      "php": "8.0.2"
    },
    "allow-plugins": {
      "composer/installers": true
    }
  }
}
