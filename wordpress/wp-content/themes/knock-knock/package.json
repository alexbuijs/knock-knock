{
  "name": "knock-knock",
  "version": "0.1.0",
  "description": "Knock Knock, a Cohousing Intranet Wordpress Theme",
  "scripts": {
    "start": "npm run dev",
    "dev": "NODE_ENV=development webpack --hide-modules",
    "build": "npm run production",
    "prod": "npm run production",
    "production": "NODE_ENV=production npm run clean && webpack --hide-modules",
    "clean": "rimraf dist",
    "test": "npm-run-all stylelint eslint",
    "stylelint": "stylelint 'assets/scss/**/*.scss'",
    "stylelint:fix": "stylelint 'assets/scss/**/*.scss' --fix",
    "eslint": "eslint 'assets/js/**'",
    "eslint:fix": "eslint 'assets/js/**' --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/alexbuijs/knock-knock.git"
  },
  "keywords": [
    "wordpress",
    "theme",
    "cohousing"
  ],
  "browserslist": {
    "production": [
      "defaults"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version"
    ]
  },
  "author": "Klopvaart Internet Team",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/alexbuijs/knock-knock/issues"
  },
  "homepage": "https://github.com/alexbuijs/knock-knock#readme",
  "stylelint": {
    "extends": "stylelint-config-sass-guidelines",
    "rules": {
      "max-nesting-depth": 3
    }
  },
  "eslintConfig": {
    "extends": "standard"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@prettier/plugin-php": "^0.14.2",
    "autoprefixer": "^9.8.5",
    "babel-loader": "^8.1.0",
    "browser-sync": "^2.26.10",
    "browser-sync-webpack-plugin": "^2.2.2",
    "css-loader": "^3.6.0",
    "eslint": "^7.4.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^9.0.2",
    "stylelint": "^13.6.1",
    "stylelint-config-sass-guidelines": "^7.0.0",
    "stylelint-webpack-plugin": "^2.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-manifest-plugin": "^2.2.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.14.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-regular-svg-icons": "^5.14.0",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@wordpress/dom-ready": "^2.10.0",
    "bootstrap": "^4.5.0",
    "flatpickr": "^4.6.3",
    "jquery": "^3.5.1",
    "popper.js": "^1.16.1",
    "promise.prototype.finally": "^3.1.2",
    "whatwg-fetch": "^3.4.0"
  }
}
