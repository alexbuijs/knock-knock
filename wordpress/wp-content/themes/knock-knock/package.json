{
  "name": "knock-knock",
  "version": "0.1.0",
  "description": "Knock Knock, a Cohousing Intranet Wordpress Theme",
  "scripts": {
    "start": "npm run dev",
    "dev": "NODE_ENV=development webpack --hide-modules",
    "build": "npm run production",
    "prod": "npm run production",
    "production": "NODE_ENV=production npm run clean && webpack --hide-modules",
    "clean": "rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/alexbuijs/knock-knock.git"
  },
  "keywords": [
    "wordpress",
    "theme",
    "cohousing"
  ],
  "browserslist": {
    "production": [
      "defaults"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version"
    ]
  },
  "author": "Klopvaart Internet Team",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/alexbuijs/knock-knock/issues"
  },
  "homepage": "https://github.com/alexbuijs/knock-knock#readme",
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/preset-env": "^7.7.6",
    "autoprefixer": "^9.7.3",
    "babel-loader": "^8.0.6",
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "^2.2.2",
    "css-loader": "^3.3.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.13.0",
    "postcss-loader": "^3.0.0",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-manifest-plugin": "^2.2.0"
  },
  "dependencies": {}
}
