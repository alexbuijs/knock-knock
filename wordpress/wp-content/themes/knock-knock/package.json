{
  "name": "knock-knock",
  "version": "0.1.0",
  "description": "Knock Knock, a Cohousing Intranet Wordpress Theme",
  "scripts": {
    "start": "npm run dev",
    "dev": "NODE_ENV=development webpack --hide-modules",
    "build": "npm run production",
    "prod": "npm run production",
    "production": "NODE_ENV=production npm run clean && webpack --hide-modules",
    "clean": "rimraf dist",
    "test": "npm run stylelint",
    "stylelint": "stylelint 'assets/scss/**/*.scss'",
    "stylelint:fix": "stylelint 'assets/scss/**/*.scss' --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/alexbuijs/knock-knock.git"
  },
  "keywords": [
    "wordpress",
    "theme",
    "cohousing"
  ],
  "browserslist": {
    "production": [
      "defaults"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version"
    ]
  },
  "author": "Klopvaart Internet Team",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/alexbuijs/knock-knock/issues"
  },
  "homepage": "https://github.com/alexbuijs/knock-knock#readme",
  "stylelint": {
    "extends": "stylelint-config-sass-guidelines",
    "rules": {
      "max-nesting-depth": 3
    }
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@prettier/plugin-php": "^0.13.0",
    "autoprefixer": "^9.7.4",
    "babel-loader": "^8.1.0",
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "^2.2.2",
    "css-loader": "^3.4.2",
    "mini-css-extract-plugin": "^0.8.2",
    "node-sass": "^4.13.1",
    "postcss-loader": "^3.0.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "stylelint": "^13.2.1",
    "stylelint-config-sass-guidelines": "^7.0.0",
    "stylelint-webpack-plugin": "^1.2.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-manifest-plugin": "^2.2.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.12.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-regular-svg-icons": "^5.12.1",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "@wordpress/dom-ready": "^2.7.0",
    "bootstrap": "^4.4.1",
    "flatpickr": "^4.6.3",
    "jquery": "^3.4.1",
    "popper.js": "^1.16.1"
  }
}
